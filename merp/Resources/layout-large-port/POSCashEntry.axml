<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:gravity="center"
        android:background="@android:drawable/editbox_dropdown_light_frame">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:minWidth="25px"
            android:minHeight="25px">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@android:drawable/editbox_dropdown_light_frame"
                android:id="@+id/linearLayout10">
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:orientation="horizontal"
                    android:weightSum="5"
                    android:id="@+id/linearLayoutCS">
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.5"
                        android:padding="2dip"
                        android:id="@+id/linearLayout2">
                        <TextView
                            android:textSize="18sp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/txtInvno"
                            android:editable="false"
                            android:textColor="#ff093bf6"
                            android:paddingLeft="5dp"
                            android:background="@drawable/bluethem_textfield_disabled_holo_light"
                            android:hint="@string/invform_invno" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:padding="2dip"
                        android:id="@+id/linearLayout2">
                        <EditText
                            android:textSize="18sp"
                            android:inputType="date"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:background="@drawable/bluethem_edit_text_holo_light"
                            android:id="@+id/txtInvdate"
                            android:textColor="#ff093bf6" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.0"
                        android:padding="2dip"
                        android:id="@+id/linearLayout2">
                        <TextView
                            android:textSize="18sp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/txtInvmode"
                            android:textColor="@android:color/holo_red_dark"
                            android:editable="false"
                            android:paddingLeft="5dp"
                            android:background="@drawable/bluethem_textfield_disabled_holo_light" />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:orientation="horizontal"
                    android:weightSum="3"
                    android:id="@+id/linearLayout1">
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.7"
                        android:padding="2dip"
                        android:id="@+id/linearLayout2">
                        <Spinner
                            android:textSize="14sp"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/custcode"
                            android:paddingLeft="5dp"
                            android:background="@drawable/bluethem_spinner_background_holo_light" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="fill_parent"
                        android:layout_weight="0.3"
                        android:gravity="center"
                        android:paddingLeft="5dip"
                        android:id="@+id/linearLayout3">
                        <Button
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#ffffff"
                            android:id="@+id/bfindCust"
                            android:background="@android:drawable/ic_search_category_default" />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:orientation="horizontal"
                    android:weightSum="3"
                    android:id="@+id/linearLayout4">
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:padding="2dip"
                        android:id="@+id/linearLayout5">
                        <Spinner
                            android:textSize="16sp"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/txtcode"
                            android:paddingLeft="5dp"
                            android:background="@drawable/bluethem_spinner_background_holo_light"
                            android:fitsSystemWindows="false" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="fill_parent"
                        android:layout_weight=".7"
                        android:gravity="center"
                        android:paddingLeft="5dip"
                        android:id="@+id/linearLayout6">
                        <EditText
                            android:textSize="18sp"
                            android:inputType="text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/txtbarcode"
                            android:paddingLeft="5dp"
                            android:textColor="#ff140000"
                            android:background="@drawable/bluethem_edit_text_holo_light"
                            android:hint="@string/title_barcode"
                            android:textColorHint="@color/xam_gray" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="fill_parent"
                        android:layout_weight="0.3"
                        android:gravity="center"
                        android:paddingLeft="5dip"
                        android:id="@+id/linearLayout6">
                        <Button
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#ffffff"
                            android:id="@+id/bfindItem"
                            android:background="@android:drawable/ic_search_category_default" />
                    </LinearLayout>
                </LinearLayout>
                <TableLayout
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/tableLayout1">
                    <TableRow
                        android:id="@+id/tableRow1">
                        <TextView
                            android:text="@string/list_qty"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="#fff6092b"
                            android:paddingLeft="5dp"
                            android:id="@+id/textView4"
                            android:textSize="18sp"
                            android:width="50dp" />
                        <EditText
                            android:textSize="18sp"
                            android:inputType="numberDecimal|numberSigned"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/txtqty"
                            android:paddingLeft="5dp"
                            android:width="120dp"
                            android:textColor="#ff140000"
                            android:background="@drawable/bluethem_edit_text_holo_light"
                            android:textColorHint="@color/xam_gray" />
                        <TextView
                            android:text="@string/list_price"
                            android:textSize="18sp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="#ff093bf6"
                            android:paddingLeft="5dp"
                            android:id="@+id/textView5"
                            android:width="70dp" />
                        <EditText
                            android:textSize="18sp"
                            android:inputType="numberDecimal|numberSigned"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/txtprice"
                            android:textColor="#ff140000"
                            android:paddingLeft="5dp"
                            android:width="120dp"
                            android:background="@drawable/bluethem_edit_text_holo_light"
                            android:editable="false"
                            android:enabled="true" />
                        <TextView
                            android:textSize="18sp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/txttax"
                            android:textColor="#ff140000"
                            android:paddingLeft="5dp"
                            android:background="@drawable/bluethem_textfield_disabled_holo_light"
                            android:selectAllOnFocus="false"
                            android:editable="false"
                            android:width="90dp" />
                    </TableRow>
                </TableLayout>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:weightSum="7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/action_bar_title_color"
                android:id="@+id/linearLayout1">
                <TextView
                    android:text="@string/list_itemcode"
                    android:textSize="18sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="2.3"
                    android:singleLine="true"
                    android:textColor="#ffffff"
                    android:typeface="sans" />
                <TextView
                    android:textSize="18sp"
                    android:text="@string/list_qty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.6"
                    android:singleLine="true"
                    android:textColor="#ffffff"
                    android:gravity="right"
                    android:textAlignment="gravity"
                    android:typeface="sans" />
                <TextView
                    android:textSize="18sp"
                    android:text="@string/list_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:singleLine="true"
                    android:gravity="right"
                    android:textColor="#ffffff"
                    android:textAlignment="gravity"
                    android:typeface="sans" />
                <TextView
                    android:textSize="12sp"
                    android:text=""
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:singleLine="true"
                    android:gravity="right"
                    android:textColor="#ffffff"
                    android:textAlignment="gravity"
                    android:typeface="sans" />
                <TextView
                    android:textSize="15sp"
                    android:text="@string/list_taxamt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.2"
                    android:singleLine="true"
                    android:textColor="#ffffff"
                    android:gravity="right"
                    android:textAlignment="gravity"
                    android:typeface="sans" />
                <TextView
                    android:textSize="15sp"
                    android:text="@string/list_amount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.3"
                    android:singleLine="true"
                    android:textColor="#ffffff"
                    android:gravity="right"
                    android:textAlignment="gravity"
                    android:typeface="sans"
                    android:paddingRight="3dp" />
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/footer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:gravity="center">
        <TableLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:height="30dp"
            android:id="@+id/tableLayout1">
            <TableRow
                android:id="@+id/tableRow1"
                android:layout_height="match_parent">
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ffffff"
                    android:typeface="sans"
                    android:textSize="18sp"
                    android:id="@+id/Save"
                    android:text="@string/button_add"
                    android:drawableLeft="@drawable/add"
                    android:width="130dp" />
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ffffff"
                    android:typeface="sans"
                    android:textSize="18sp"
                    android:text="@string/button_Paid"
                    android:id="@+id/Paid"
                    android:drawableLeft="@drawable/shop"
                    android:width="130dp" />
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ffffff"
                    android:typeface="sans"
                    android:textSize="18sp"
                    android:id="@+id/NewBill"
                    android:text="@string/button_new"
                    android:drawableLeft="@drawable/bill"
                    android:width="130dp" />
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ffffff"
                    android:typeface="sans"
                    android:textSize="18sp"
                    android:text="@string/button_cancel"
                    android:id="@+id/Cancel"
                    android:drawableLeft="@android:drawable/ic_delete"
                    android:width="130dp" />
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ffffff"
                    android:typeface="sans"
                    android:id="@+id/List"
                    android:textSize="18sp"
                    android:text="BILLS"
                    android:drawableLeft="@drawable/shoplist"
                    android:width="130dp" />
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ffffff"
                    android:typeface="sans"
                    android:id="@+id/Print"
                    android:textSize="18sp"
                    android:text="@string/button_print"
                    android:drawableLeft="@drawable/print"
                    android:width="130dp" />
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ffffff"
                    android:typeface="sans"
                    android:textSize="18sp"
                    android:text="@string/submenu_back"
                    android:id="@+id/Home"
                    android:drawableLeft="@drawable/home"
                    android:width="130dp" />
            </TableRow>
        </TableLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/content"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_above="@id/footer"
        android:layout_below="@id/header">
        <ListView
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:id="@+id/invitemList"
            android:layout_marginBottom="0.0dp" />
    </RelativeLayout>
</RelativeLayout>